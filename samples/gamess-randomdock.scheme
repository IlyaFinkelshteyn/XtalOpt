[randomdock]
edit\optType=0
optimizer\GAMESS\job.inp_list="%NProcShared=2\n#n PM3 Opt\n\n%filename%\n\n0 1\n%coords%\n\n"
optimizer\GAMESS\job.pbs_list="#/bin/bash                                   \n#PBS -q em64t                                \n#PBS -l walltime=2:00:00\n#PBS -l nodes=1:ppn=2                                            \n#PBS -o %rempath%/out.g03\n#PBS -N %description%-%id%\n#PBS -j oe\n#PBS -m e\n##PBS -W umask=022\n\n###Include this for xtalopt scripts!###\nexport PBS_O_WORKDIR=%rempath%   \necho $PBS_O_WORKDIR\ncd $PBS_O_WORKDIR\n# -----------------------------------------------------------------------\n# start: prepare script to clean up scratch directories\n# -----------------------------------------------------------------------\nCLEANUPSCRIPT=~/pbscleanup_$PBS_JOBID.sh\necho \"# clean up job $PBS_JOBID\" >$CLEANUPSCRIPT\nfor i in `cat $PBS_NODEFILE | sort | uniq`; do\necho \"ssh $i \\\"rm -r $PBSTMPDIR\\\" \" >> $CLEANUPSCRIPT\ndone\nchmod +x $CLEANUPSCRIPT\n# -----------------------------------------------------------------------\n# end:  prepare script to clean up scratch directories\n# -----------------------------------------------------------------------\n\ncd $PBS_O_WORKDIR\nfor node in `cat $PBS_NODEFILE | sort | uniq`; do\nrsh $node \"cp $PBS_O_WORKDIR/* $PBSTMPDIR/;\";\ndone\ncd $PBSTMPDIR\necho \"running in directory $PBSTMPDIR\"\n###########\n# End PBS setup. User comands start below.\n# Do NOT redirect standard output. It will all be collected in\n# the PBS output file specified in the header above.\n###########\n\nsource /software/gaussian/tag.sh\ng03 < input.g03\n\nfor node in `cat $PBS_NODEFILE | sort | uniq`; do\nrsh $node \"ls -l $PBSTMPDIR\"\ndone\ncp $PBSTMPDIR/* $PBS_O_WORKDIR/\n"
optimizer\GAMESS\user1=
optimizer\GAMESS\user2=
optimizer\GAMESS\user3=
optimizer\GAMESS\user4=
edit\version=1
